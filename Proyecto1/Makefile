Main:
	gcc -Wall -g mainP.c -o friendfindP
	gcc -Wall -g mainH.c -pthread -o friendfindH
	gcc -Wall -g map.c -o map
	gcc -Wall -g reduce.c -o reduce

clean:
	rm friendfindP
	rm friendfindH
	rm map
	rm reduce
